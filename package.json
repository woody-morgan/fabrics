{
  "name": "fabricate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky .husky",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "pnpm build && npx vite-bundle-visualizer",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "type-coverage && vitest run --coverage",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "check:unimported": "npx unimported --include \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@mobily/ts-belt": "^3.13.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-pattern": "^5.0.8"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.20.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-unused-imports": "^3.1.0",
    "husky": ">=7",
    "jsdom": "^24.0.0",
    "lint-staged": ">=10",
    "prettier": "^3.2.5",
    "type-coverage": "^2.27.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.14",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.1",
    "vitest-canvas-mock": "^0.3.3"
  },
  "packageManager": "pnpm@8.6.2",
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss, md}": [
      "prettier --write"
    ]
  },
  "typeCoverage": {
    "atLeast": 95,
    "project": "tsconfig.json",
    "strict": true,
    "ignoreTypeAssertion": true,
    "ignoreCatch": true,
    "ignoreFiles": [
      "src/api-gen/**/*.{ts,tsx}"
    ],
    "ignoreObject": true,
    "reportSemanticError": true
  }
}
